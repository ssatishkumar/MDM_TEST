	<test name="HeiPort_Login_LDR">
		<classes>
			<class name="com.mendix.test.LoginScript">
				<methods>
					<include name="tasKill" />
					<include name="openBrowser" />
					<include name="loginAsLDR" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="Material_Create_Fill in questionnaire">
